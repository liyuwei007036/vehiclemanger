<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- slide bar 有效内容背景 -->
    <attr name="backgroundColor" format="color|reference" />
    <!-- slide bar　描边 -->
    <attr name="strokeColor" format="color|reference" />
    <!-- slide bar 文字颜色 -->
    <attr name="textColor" format="color|reference" />
    <!-- slide bar　文字大小 -->
    <attr name="textSize" format="dimension" />
    <!-- slide bar 选中文字颜色 -->
    <attr name="selectTextColor" format="color|reference" />
    <!-- slide bar 选中文字大小 -->
    <attr name="selectTextSize" format="dimension" />
    <!-- slide bar　选中时候提示文字颜色 -->
    <attr name="hintTextColor" format="color|reference" />
    <!-- slide bar　选中时候提示文字大小 -->
    <attr name="hintTextSize" format="dimension" />
    <!-- slide bar　选中时候提示圆形半径 -->
    <attr name="hintCircleRadius" format="dimension" />
    <!-- slide bar　选中时候提示圆形颜色 -->
    <attr name="hintCircleColor" format="color|reference" />
    <!-- slide bar　选中时候提示背景形状 -->
    <attr name="hintShape" format="enum">
        <enum name="circle" value="0" />
        <enum name="rectangle" value="1" />
    </attr>
    <!-- slide bar　选中时候波浪颜色 -->
    <attr name="waveColor" format="color|reference" />
    <!-- slide bar　选中时候波浪半径 -->
    <attr name="waveRadius" format="dimension" />
    <!-- slide bar　里面文字上＋下padding -->
    <attr name="contentPadding" format="dimension" />
    <!-- slide bar　上+下+右padding -->
    <attr name="barPadding" format="dimension" />
    <!-- slide bar　宽度 -->
    <attr name="barWidth" format="dimension" />
    <declare-styleable name="AZWaveSideBarView">
        <!-- slide bar 有效内容背景 -->
        <attr name="backgroundColor" />
        <!-- slide bar　描边 -->
        <attr name="strokeColor" />
        <!-- slide bar 文字颜色 -->
        <attr name="textColor" />
        <!-- slide bar　文字大小 -->
        <attr name="textSize" />
        <!-- slide bar 选中文字颜色 -->
        <attr name="selectTextColor" />
        <!-- slide bar 选中文字大小 -->
        <attr name="selectTextSize" />
        <!-- slide bar　选中时候提示文字颜色 -->
        <attr name="hintTextColor" />
        <!-- slide bar　选中时候提示文字大小 -->
        <attr name="hintTextSize" />
        <!-- slide bar　选中时候提示圆形半径 -->
        <attr name="hintCircleRadius" />
        <!-- slide bar　选中时候提示圆形颜色 -->
        <attr name="hintCircleColor" />
        <!-- slide bar　选中时候波浪颜色 -->
        <attr name="waveColor" />
        <!-- slide bar　选中时候波浪半径 -->
        <attr name="waveRadius" />
        <!-- slide bar　里面文字上＋下padding -->
        <attr name="contentPadding" />
        <!-- slide bar　上+下+右padding -->
        <attr name="barPadding" />
        <!-- slide bar　宽度 -->
        <attr name="barWidth" />
    </declare-styleable>

    <declare-styleable name="SideBarView">
        <!-- slide bar 有效内容背景 -->
        <attr name="backgroundColor" />
        <!-- slide bar　描边 -->
        <attr name="strokeColor" />
        <!-- slide bar 文字颜色 -->
        <attr name="textColor" />
        <!-- slide bar　文字大小 -->
        <attr name="textSize" />
        <!-- slide bar 选中文字颜色 -->
        <attr name="selectTextColor" />
        <!-- slide bar 选中文字大小 -->
        <attr name="selectTextSize" />
        <!-- slide bar　选中时候提示文字颜色 -->
        <attr name="hintTextColor" />
        <!-- slide bar　选中时候提示文字大小 -->
        <attr name="hintTextSize" />
        <!-- slide bar　选中时候提示圆形半径 -->
        <attr name="hintCircleRadius" />
        <attr name="hintShape" />
        <!-- slide bar　选中时候提示圆形颜色 -->
        <attr name="hintCircleColor" />
        <!-- slide bar　里面文字上＋下padding -->
        <attr name="contentPadding" />
        <!-- slide bar　上+下+右padding -->
        <attr name="barPadding" />
        <!-- slide bar　宽度 -->
        <attr name="barWidth" />

        <attr name="textHeight" />

        <attr name="hintBackGround" />

        <attr name="hintHeight" />

    </declare-styleable>


    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer" />
        <attr name="stroke_width" format="dimension" />
        <attr name="color_tick" format="color" />
        <attr name="color_checked" format="color" />
        <attr name="color_unchecked" format="color" />
        <attr name="color_unchecked_stroke" format="color" />
    </declare-styleable>


    <declare-styleable name="TextArea">
        <!--设置默认文字-->
        <attr name="etText" format="string" />
        <!--设置提示文字-->
        <attr name="etHint" format="string" />
        <!--设置提示文字颜色-->
        <attr name="etHintColor" format="color" />
        <!--设置最小高度-->
        <attr name="etMinHeight" format="dimension" />
        <!--设置最大长度-->
        <attr name="etMaxLength" format="integer" />
        <!--设置横线颜色-->
        <attr name="etLineColor" format="color" />
        <!--设置输入文字大小-->
        <attr name="etTextSize" format="dimension" />
        <!--设置输入文字颜色-->
        <attr name="etTextColor" format="color" />
        <!--设置提示统计文字大小-->
        <attr name="etPromptTextSize" format="dimension" />
        <!--设置提示统计文字颜色-->
        <attr name="etPromptTextColor" format="color" />
        <!--设置提示统计显示类型-->
        <attr name="etType">
            <!--单数类型-->
            <enum name="singular" value="0" />
            <!--百分比类型-->
            <enum name="percentage" value="1" />
        </attr>
        <!--设置提示统计显示类型位置-->
        <attr name="etPromptPosition">
            <!--上方-->
            <enum name="promptUp" value="0" />
            <!--下方-->
            <enum name="promptDn" value="1" />
        </attr>
    </declare-styleable>
</resources>